<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_centerHorizontal="false"
    android:layout_marginStart="10dp"
    app:layout_anchorGravity="center_horizontal"
    tools:context=".ViewProfileActivity">

    <ImageView
        android:id="@+id/logoView"
        android:layout_width="169dp"
        android:layout_height="163dp"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="15dp"
        android:contentDescription="Logo creado Por Ricardo Flores"
        app:srcCompat="@drawable/vector_ugb" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/logoView"
        android:layout_marginTop="25dp"
        android:paddingLeft="5dp"
        android:text="Nombre de Contacto" />

    <EditText
        android:id="@+id/txtViewName"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView2"
        android:layout_marginTop="15dp"
        android:ems="10"
        android:hint="Nombre Completo"
        android:inputType="text"
        android:minHeight="48dp"
        android:paddingLeft="10dp"
        android:editable="false"
        android:textColorHint="#0C0C0C"
        tools:ignore="VisualLintTextFieldSize"
        android:textColor="#000000"
        android:backgroundTint="#FFFFFF"
        android:enabled="false" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/txtViewName"
        android:layout_marginTop="25dp"
        android:autofillHints=""
        android:importantForAutofill="no"
        android:paddingLeft="5dp"
        android:text="NÃºmero de Contacto"
        tools:ignore="RtlSymmetry" />

    <EditText
        android:id="@+id/txtViewPhone"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView4"
        android:layout_marginTop="15dp"
        android:ems="10"
        android:hint="0000-0000"
        android:inputType="phone"
        android:minHeight="48dp"
        android:paddingLeft="10dp"
        android:textColorHint="#0C0C0C"
        android:editable="false"
        tools:ignore="VisualLintTextFieldSize"
        android:textColor="#000000"
        android:backgroundTint="#FFFFFF"/>

    <Button
        android:id="@+id/btnRegresar"
        android:layout_width="wrap_content"
        android:layout_height="59dp"
        android:layout_below="@+id/txtViewPhone"
        android:layout_alignParentEnd="true"
        android:layout_marginLeft="260dp"
        android:layout_marginTop="31dp"
        android:layout_marginEnd="35dp"
        android:text="Regresar" />

    <Button
        android:id="@+id/btnLlamar"
        android:layout_width="116dp"
        android:layout_height="59dp"
        android:layout_below="@+id/txtViewPhone"
        android:layout_alignParentStart="true"
        android:layout_marginStart="35dp"
        android:layout_marginTop="31dp"
        android:text="Llamar" />
</RelativeLayout>
